<h1>Listing company_laptops</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Laptop</th>
    <th>Cantidad</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @company_laptops.each do |company_laptop| %>
  <tr>
    <td><%= company_laptop.company_id %></td>
    <td><%= company_laptop.laptop_id %></td>
    <td><%= company_laptop.cantidad %></td>
    <td><%= link_to 'Show', company_laptop %></td>
    <td><%= link_to 'Edit', edit_company_laptop_path(company_laptop) %></td>
    <td><%= link_to 'Destroy', company_laptop, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

